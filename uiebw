        Icon = "crown",
        Author = "wbwhnb (QQ: 2318995920)",
        Folder = "wSuperScript",
        Size = UDim2.fromOffset(600, 400),
        Theme = "Dark", -- æ·±è‰²ä¸»é¢˜æ›´é€‚é…æ¸¸æˆè¾…åŠ©åœºæ™¯
        User = {
            Enabled = true,
            Anonymous = true,
