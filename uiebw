        HideSearchBar = false,
    })

    -- ç¼–è¾‘çª—å£æ‰“å¼€æŒ‰é’®ï¼ˆæ¸å˜æ ·å¼ï¼‰
    MainWindow:EditOpenButton({
        Title = "w è¶…çº§è„šæœ¬",
        Icon = "crown",
        CornerRadius = UDim.new(0, 10),
        StrokeThickness = 2,
        Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, Color3.fromHex("#FF0000")),
            ColorSequenceKeypoint.new(0.5, Color3.fromHex("#FFFF00")),
            ColorSequenceKeypoint.new(1, Color3.fromHex("#00FF00"))
        }),
        Draggable = true,
    })

    -- ==================== ä½œè€…ä¿¡æ¯ Tab ====================
    local AuthorTab = MainWindow:Tab({ Title = "ä½œè€…ä¿¡æ¯", Icon = "user" })
    AuthorTab:Section({ Title = "è„šæœ¬ä½œè€…è¯¦æƒ…" })
    AuthorTab:Paragraph({ Text = "ä½œè€…: wbwhnb" })
    AuthorTab:Paragraph({ Text = "ä½œè€… QQ: 2318995920" })
    AuthorTab:Paragraph({ Text = "è„šæœ¬æ€§è´¨: å…è´¹å¼€æº" })
    AuthorTab:Button({
        Title = "å¤åˆ¶ä½œè€… QQ",
        Icon = "copy",
        Callback = function()
            setclipboard("2318995920")
            WindUI:Notify({ Title = "æç¤º", Content = "QQ å·²å¤åˆ¶åˆ°å‰ªè´´æ¿", Duration = 3 })
        end
    })

    -- ==================== ç©å®¶ä¿¡æ¯ Tab ====================
    local PlayerTab = MainWindow:Tab({ Title = "ç©å®¶ä¿¡æ¯", Icon = "info" })
    PlayerTab:Section({ Title = "å½“å‰æ¸¸æˆä¿¡æ¯" })
    PlayerTab:Paragraph({ Text = "ç”¨æˆ·å: " .. LocalPlayer.Name })
    PlayerTab:Paragraph({ Text = "æ³¨å…¥å™¨: " .. (identifyexecutor() or "æœªçŸ¥") })
    PlayerTab:Paragraph({ Text = "æœåŠ¡å™¨ ID: " .. game.PlaceId })
    PlayerTab:Paragraph({ Text = "æ¸¸æˆåç§°: " .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name })

    -- ==================== å…¬å‘Š Tab ====================
    local NoticeTab = MainWindow:Tab({ Title = "è„šæœ¬å…¬å‘Š", Icon = "bell" })
    NoticeTab:Section({ Title = "é‡è¦æç¤º" })
    NoticeTab:Paragraph({ Text = "ğŸ“¢ æ­¤è„šæœ¬ä¸ºå…è´¹è„šæœ¬ï¼Œä¸¥ç¦å€’å–åœˆé’±" })
